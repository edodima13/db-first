SELECT COUNT(*), YEAR(`enrolment_date`) 
FROM `students` 
GROUP BY YEAR(`enrolment_date`);

--------------------------------------

SELECT COUNT(*), `office_address` 
FROM `teachers` 
GROUP BY `office_address`;

--------------------------------------

SELECT AVG(`vote`), `exam_id` 
FROM `exam_student` 
GROUP BY `exam_id`;

--------------------------------------
