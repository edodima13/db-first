SELECT * 
FROM `students` 
WHERE YEAR(`date_of_birth`) = 1990

--------------------------------------

SELECT * 
FROM `courses` 
WHERE `cfu` > 10

--------------------------------------

SELECT * 
FROM `students` 
WHERE TIMESTAMPDIFF(YEAR,`date_of_birth`, CURRENT_DATE()) > 30

--------------------------------------

SELECT * 
FROM `courses` 
WHERE `year` = 1 
AND `period` LIKE 'I %'

--------------------------------------

SELECT * 
FROM `exams` 
WHERE `date` = '2020-06-20' 
AND HOUR(`hour`) > 14

--------------------------------------